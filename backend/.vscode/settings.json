{
    "peacock.remoteColor": "#ff774d",
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        "**/.DS_Store": false,
        "**/Thumbs.db": false,
        "runtime.txt": true,
        "requirements.txt": true,
        "Procfile": true,
        "dump.rdb": true,
        "db.sqlite3": true,
        "**/.gitignore": false,
        "**/venv": false,
        "venv": true,
        "**/.vscode": false,
        ".vscode": true,
        "**/static": false,
        "static": true,
        "manage.py": true,
        ".gitignore": true,
        "__pycache__": true,
        "migrations": true,
        "ai": true
    },
    "explorerExclude.backup": {},
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#ff9e80",
        "activityBar.background": "#ff9e80",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#00801e",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#15202b99",
        "sash.hoverBorder": "#ff9e80",
        "statusBar.background": "#ff774d",
        "statusBar.foreground": "#15202b",
        "statusBarItem.hoverBackground": "#ff501a",
        "statusBarItem.remoteBackground": "#ff774d",
        "statusBarItem.remoteForeground": "#15202b",
        "titleBar.activeBackground": "#ff774d",
        "titleBar.activeForeground": "#15202b",
        "titleBar.inactiveBackground": "#ff774d99",
        "titleBar.inactiveForeground": "#15202b99"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "python.formatting.provider": "none"
}